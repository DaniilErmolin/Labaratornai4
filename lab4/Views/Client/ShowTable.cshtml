@{
    ViewData["Title"] = "Клиенты";
    @model List<Client>;

}

<div class="text-center">
    <table class="table">
        <tr>
            <th>Id</th>
            <th>ФИО</th>
            <th>Дата рождения</th>
            <th>Пол</th>
            <th>Адрес</th>
            <th>Серия паспорта</th>
            <th>Номер паспорта</th>
            <th>Скидка</th>
        </tr>
        @foreach (var clients in Model)
        {
            <tr>
                <td>
                    @clients.Id
                </td>
                <td>
                    @clients.Fio
                </td>
                <td>
                    @clients.DateOfBirth
                </td>
                <td>
                    @clients.Sex
                </td>
                <td>
                    @clients.Address
                </td>
                <td>
                    @clients.Series
                </td>
                <td>
                    @clients.Number
                </td>
                <td>
                    @clients.Discount
                </td>
            </tr>
        }
    </table>
</div>
